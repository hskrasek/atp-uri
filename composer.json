{
    "name": "hskrasek/atp-uri",
    "description": "A PHP Implementation for parsing and generating at:// uris",
    "keywords": [
        "hskrasek",
        "ATProtocol"
    ],
    "homepage": "https://github.com/hskrasek/atp-uri",
    "license": "MIT",
    "authors": [
        {
            "name": "Hunter Skrasek",
            "email": "hunterskrasek@me.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "HSkrasek\\ATP\\Uri\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HSkrasek\\ATP\\Uri\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
